! General
! URxvt
URxvt*depth:            0
URxvt*shading:          20
URxvt*transparent:      false
URxvt*transparency:     true
URxvt.scrollstyle:      rxvt
URxvt.scrollBar:        off
URxvt.letterSpace:      0
URxvt.imLocale:         en_US.UTF-8

*cursorBlink:           True
*.cursorUnderline:      False
URxvt.termName:         xterm
URxvt.internalBorder:   2


! Fonts
Xft*dpi:                96
Xft*antalias:           true
Xft*hinting:            full

URxvt*font:         xft:tamsyn:pixelsize=16
URxvt*boldFont:     xft:tamsyn:pixelsize=16


